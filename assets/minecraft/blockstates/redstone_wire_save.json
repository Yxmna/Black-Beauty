{
	"multipart": [
		{
			"when": {
				"OR": [
					{
				"north": "side|up",
				"south": "side|up",
				"east": "side|up",
				"west": "side|up"
					},
					{
				"north": "none",
				"south": "none",
				"east": "none",
				"west": "none"
					}
				]
			},
			"apply": {
				"model": "minecraft:block/redstone_dust_dot"
			}
		},
		{
			"when": {
				"north": "side|up"
			},
			"apply": {
				"model": "minecraft:block/redstone_dust_side"
			}
		},
		{
			"when": {
				"south": "side|up"
			},
			"apply": {
				"model": "minecraft:block/redstone_dust_side",
				"y": 180
			}
		},
		{
			"when": {
				"east": "side|up"
			},
			"apply": {
				"model": "minecraft:block/redstone_dust_side",
				"y": 90
			}
		},
		{
			"when": {
				"west": "side|up"
			},
			"apply": {
				"model": "minecraft:block/redstone_dust_side",
				"y": 270
			}
		},
		{
			"when": {
				"north": "up"
			},
			"apply": {
				"model": "minecraft:block/redstone_dust_up"
			}
		},
		{
			"when": {
				"south": "up"
			},
			"apply": {
				"model": "minecraft:block/redstone_dust_up",
				"y": 180
			}
		},
		{
			"when": {
				"east": "up"
			},
			"apply": {
				"model": "minecraft:block/redstone_dust_up",
				"y": 90
			}
		},
		{
			"when": {
				"west": "up"
			},
			"apply": {
				"model": "minecraft:block/redstone_dust_up",
				"y": 270
			}
		}
	]
}












